{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ostler",
  "version": "0.1.0",
  "identifier": "com.ostler.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "ostler",
        "width": 1200,
        "height": 800
      }
    ]
  }
}
